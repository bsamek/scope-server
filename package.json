{
  "name": "scout-server",
  "description": "Uses the nodejs native driver to provide an http api to mongodb",
  "version": "0.0.4",
  "scripts": {
    "ci": "gulp ci",
    "test": "mocha",
    "start": "nodemon --no-stdin --watch lib bin/scout-server.js",
    "check": "mongodb-js-precommit",
    "security-audit": "nsp package",
    "coverage": "./node_modules/istanbul/lib/cli.js cover _mocha"
  },
  "check": {
    "ignore": [
      "coverage/{*,**/*}"
    ]
  },
  "precommit": [
    "check",
    "security-audit"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/10gen/scout-server.git"
  },
  "bin": {
    "scout-server": "bin/scout-server.js"
  },
  "dependencies": {
    "async": "^1.3.0",
    "body-parser": "^1.13.1",
    "boom": "^2.8.0",
    "csv-write-stream": "^1.0.0",
    "debug": "^2.2.0",
    "event-source-emitter": "0.0.2",
    "event-stream": "^3.3.1",
    "express": "^4.13.0",
    "get-mongodb-version": "0.0.1",
    "joi": "^6.5.0",
    "jsonwebtoken": "^5.0.4",
    "lodash": "^3.10.0",
    "longest-common-substring": "0.0.1",
    "mongodb": "^2.0.35",
    "mongodb-deployment-model": "^0.2.0",
    "mongodb-extended-json": "^1.3.1",
    "mongodb-favicon": "^0.0.1",
    "mongodb-instance-model": "^0.2.0",
    "mongodb-ns": "^1.0.0",
    "mongodb-url": "0.0.1",
    "mongoscope-config": "^0.1.0",
    "peek-stream": "^1.1.1",
    "reservoir-stream": "0.0.2",
    "semver": "^5.0.1",
    "sift": "^2.0.6",
    "socket.io": "^1.3.5",
    "socket.io-stream": "0.6.1",
    "socketio-jwt": "^4.2.0",
    "titlecase": "^1.0.2",
    "uuid": "^2.0.1"
  },
  "devDependencies": {
    "coffee-script": "^1.9.3",
    "eslint-config-mongodb-js": "^0.1.4",
    "istanbul": "^0.3.17",
    "mocha": "^2.2.5",
    "mongodb-js-precommit": "^0.1.2",
    "mongodb-runner": "^2.2.0",
    "mongodb-version-manager": "^0.7.1",
    "nodemon": "^1.3.7",
    "nsp": "^1.0.3",
    "pre-commit": "^1.0.10",
    "supertest": "^1.0.1"
  }
}
