{
  "name": "scout-server",
  "description": "Uses the nodejs native driver to provide an http api to mongodb",
  "version": "0.4.1",
  "scripts": {
    "fmt": "mongodb-js-fmt lib/{*.js,**/*.js} bin/*.js test/*.js",
    "ci": "mj-ci --verbose",
    "test": "mocha",
    "start": "NODE_ENV=development nodemon --no-stdin --watch lib bin/scout-server.js",
    "check": "mongodb-js-precommit"
  },
  "check": {
    "ignore": [
      "coverage/{*,**/*}"
    ]
  },
  "precommit": [
    "check"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/10gen/scout-server.git"
  },
  "bin": {
    "scout-server": "bin/scout-server.js"
  },
  "dependencies": {
    "async": "^1.5.0",
    "body-parser": "^1.14.1",
    "boom": "^2.10.0",
    "bugsnag": "^1.6.6",
    "chalk": "^1.1.1",
    "csv-write-stream": "^1.0.0",
    "debug": "^2.2.0",
    "event-source-emitter": "0.0.2",
    "event-stream": "^3.3.2",
    "express": "^4.13.3",
    "figures": "^1.4.0",
    "joi": "^6.9.1",
    "jsonwebtoken": "^5.4.1",
    "lodash": "^3.10.1",
    "mongodb": "^2.0.47",
    "mongodb-collection-sample": "^0.1.4",
    "mongodb-connection-model": "^3.0.4",
    "mongodb-deployment-model": "^0.2.3",
    "mongodb-extended-json": "^1.5.3",
    "mongodb-favicon": "^0.0.1",
    "mongodb-instance-model": "^0.2.4",
    "mongodb-ns": "^1.0.0",
    "mongodb-read-preference": "^1.0.4",
    "mongoscope-config": "^0.1.0",
    "sift": "^2.0.9",
    "socket.io": "^1.3.7",
    "socket.io-stream": "0.9.0",
    "socketio-jwt": "^4.3.2",
    "uuid": "^2.0.1"
  },
  "devDependencies": {
    "cli-table": "^0.3.1",
    "eslint-config-mongodb-js": "^1.0.4",
    "is-mongodb-running": "0.0.1",
    "mj": "^1.0.3",
    "mocha": "^2.3.3",
    "mongodb-js-fmt": "0.0.3",
    "mongodb-js-precommit": "^0.2.2",
    "mongodb-runner": "^3.1.11",
    "nodemon": "^1.8.0",
    "numeral": "^1.5.3",
    "pre-commit": "^1.1.2",
    "socket.io-client": "^1.3.7",
    "supertest": "^1.1.0"
  }
}